{
    "name": "kba-team/markdown-table",
    "description": "Create a markdown table string from an associative array.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor J.",
            "email": "gregor-j@users.noreply.github.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/the-kbA-team/markdown-table/issues"
    },
    "keywords": [
        "markdown",
        "table",
        "markdown-table",
        "array"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "kbATeam\\MarkdownTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\kbATeam\\MarkdownTable\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "tests": "Run all unit tests."
    }
}
